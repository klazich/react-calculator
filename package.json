{
  "name": "react-calculator",
  "private": true,
  "description": "A simple calculator built with react",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "format-root": "prettier --write \"./*.{js,jsx,json,css,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve"
  },
  "dependencies": {
    "@emotion/core": "^10.1.1",
    "@emotion/styled": "^10.0.27",
    "@mdx-js/react": "^1.6.22",
    "@rebass/preset": "^4.0.5",
    "emotion-theming": "^11.0.0",
    "gatsby": "^2.32.8",
    "gatsby-plugin-emotion": "^4.5.0",
    "gatsby-plugin-theme-ui": "^0.3.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rebass": "^4.0.7",
    "theme-ui": "^0.3.5"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-preset-gatsby": "^1.0.0",
    "eslint": "^7.21.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,md}": [
      "npm run format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
