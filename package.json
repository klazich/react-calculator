{
  "name": "react-calculator",
  "private": true,
  "description": "A simple calculator built with react",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "format-root": "prettier --write \"./*.{js,jsx,json,css,md}\"",
    "start": "yarn develop",
    "serve": "gatsby serve"
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.22",
    "@mdx-js/react": "^1.5.1",
    "@rebass/preset": "^4.0.5",
    "emotion-theming": "^10.0.19",
    "gatsby": "^2.17.2",
    "gatsby-plugin-emotion": "^4.1.12",
    "gatsby-plugin-theme-ui": "^0.2.43",
    "react": "^16.11.0",
    "react-dom": "npm:@hot-loader/react-dom",
    "rebass": "^4.0.6",
    "theme-ui": "^0.2.44"
  },
  "devDependencies": {
    "@types/react": "^16.9.11",
    "babel-eslint": "^10.0.3",
    "babel-preset-gatsby": "^0.2.20",
    "eslint": "^6.5.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "gatsby-plugin-eslint": "^2.0.5",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,md}": [
      "yarn format",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
