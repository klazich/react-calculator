{
  "name": "react-calculator",
  "private": true,
  "description": "A simple calculator built with react",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "format-root": "prettier --write \"./*.{js,jsx,json,css,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve"
  },
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@mdx-js/react": "^1.6.18",
    "@rebass/preset": "^4.0.5",
    "emotion-theming": "^10.0.19",
    "gatsby": "^2.24.73",
    "gatsby-plugin-emotion": "^4.3.14",
    "gatsby-plugin-theme-ui": "^0.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rebass": "^4.0.7",
    "theme-ui": "^0.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-preset-gatsby": "^0.5.13",
    "eslint": "^7.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "gatsby-plugin-eslint": "^2.0.8",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "prop-types": "^15.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,md}": [
      "npm run format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
